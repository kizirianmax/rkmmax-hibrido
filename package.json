{
  "name": "kizirianmax-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.8.0",
  "engines": {
    "node": "22.x",
    "npm": ">=10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:ci": "NODE_OPTIONS='--experimental-vm-modules' jest --ci --maxWorkers=2",
    "eject": "react-scripts eject",
    "lint": "eslint src/ api/ --ext .js,.jsx",
    "lint:fix": "eslint src/ api/ --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\" \"api/**/*.{js,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\" \"api/**/*.{js,json}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@sentry/react": "^10.25.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.83.0",
    "busboy": "^1.6.0",
    "form-data": "^4.0.5",
    "openai": "^4.104.0",
    "posthog-js": "^1.296.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "react-scripts": "5.0.1",
    "resend": "^6.5.0",
    "stripe": "^12.18.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^30.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "source-map-explorer": "^2.5.3"
  },
  "overrides": {
    "typescript": "5.9.3"
  }
}
