const PUBLIC_ROUTES =
/* istanbul ignore next */
(cov_27k0wv26io().s[0]++, ["/", "/plans", "/login", "/reset-password"]);
function isPublic(pathname) {
  /* istanbul ignore next */
  cov_27k0wv26io().f[0]++;
  cov_27k0wv26io().s[1]++;
