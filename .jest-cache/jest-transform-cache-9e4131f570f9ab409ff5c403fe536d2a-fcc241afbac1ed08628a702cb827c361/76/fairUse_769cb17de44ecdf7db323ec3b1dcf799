export const checkLimit = (userPlan, usage) => {
  /* istanbul ignore next */
  cov_vfpl5nwab().f[0]++;
  const plan =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[2]++, 
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[0][0]++, plans[userPlan]) ||
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[0][1]++, plans.free));
  const dailyRemaining =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[3]++, plan.limits.messagesPerDay - (
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[1][0]++, usage.messagesToday) ||
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[1][1]++, 0)));
  const monthlyRemaining =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[4]++, plan.limits.messagesPerMonth - (
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[2][0]++, usage.messagesThisMonth) ||
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[2][1]++, 0)));
  /* istanbul ignore next */
  cov_vfpl5nwab().s[5]++;
  return {
    canSendMessage: 
    /* istanbul ignore next */
    (cov_vfpl5nwab().b[3][0]++, dailyRemaining > 0) &&
    /* istanbul ignore next */
    (cov_vfpl5nwab().b[3][1]++, monthlyRemaining > 0),
    dailyRemaining,
    monthlyRemaining,
    dailyLimit: plan.limits.messagesPerDay,
    monthlyLimit: plan.limits.messagesPerMonth,
    softLimitReached: 
    /* istanbul ignore next */
    (cov_vfpl5nwab().b[4][0]++, dailyRemaining < 10) ||
    /* istanbul ignore next */
    (cov_vfpl5nwab().b[4][1]++, monthlyRemaining < 50)
  };
};

// Mensagens de aviso
/* istanbul ignore next */
cov_vfpl5nwab().s[6]++;
export const getLimitMessage = limitStatus => {
  /* istanbul ignore next */
  cov_vfpl5nwab().f[1]++;
  cov_vfpl5nwab().s[7]++;
  if (!limitStatus.canSendMessage) {
    /* istanbul ignore next */
    cov_vfpl5nwab().b[5][0]++;
    cov_vfpl5nwab().s[8]++;
    if (limitStatus.dailyRemaining <= 0) {
      /* istanbul ignore next */
      cov_vfpl5nwab().b[6][0]++;
      cov_vfpl5nwab().s[9]++;
      return {
        type: "error",
        message: "Limite diário atingido. Você poderá enviar novas mensagens amanhã ou fazer upgrade do plano.",
        action: "upgrade"
      };
    } else
    /* istanbul ignore next */
    {
      cov_vfpl5nwab().b[6][1]++;
    }
    cov_vfpl5nwab().s[10]++;
    if (limitStatus.monthlyRemaining <= 0) {
      /* istanbul ignore next */
      cov_vfpl5nwab().b[7][0]++;
      cov_vfpl5nwab().s[11]++;
      return {
        type: "error",
        message: "Limite mensal atingido. Faça upgrade para continuar usando.",
        action: "upgrade"
      };
    } else
    /* istanbul ignore next */
    {
      cov_vfpl5nwab().b[7][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_vfpl5nwab().b[5][1]++;
  }
  cov_vfpl5nwab().s[12]++;
  if (limitStatus.softLimitReached) {
    /* istanbul ignore next */
    cov_vfpl5nwab().b[8][0]++;
    cov_vfpl5nwab().s[13]++;
    if (limitStatus.dailyRemaining < 10) {
      /* istanbul ignore next */
      cov_vfpl5nwab().b[9][0]++;
      cov_vfpl5nwab().s[14]++;
      return {
        type: "warning",
        message: `Você tem apenas ${limitStatus.dailyRemaining} mensagens restantes hoje. Considere fazer upgrade.`,
        action: "info"
      };
    } else
    /* istanbul ignore next */
    {
      cov_vfpl5nwab().b[9][1]++;
    }
    cov_vfpl5nwab().s[15]++;
    if (limitStatus.monthlyRemaining < 50) {
      /* istanbul ignore next */
      cov_vfpl5nwab().b[10][0]++;
      cov_vfpl5nwab().s[16]++;
      return {
        type: "warning",
        message: `Você tem apenas ${limitStatus.monthlyRemaining} mensagens restantes este mês. Considere fazer upgrade.`,
        action: "info"
      };
    } else
    /* istanbul ignore next */
    {
      cov_vfpl5nwab().b[10][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_vfpl5nwab().b[8][1]++;
  }
  cov_vfpl5nwab().s[17]++;
  return null;
};

// Verificar se especialista está disponível no plano
/* istanbul ignore next */
cov_vfpl5nwab().s[18]++;
export const canUseSpecialist = (userPlan, specialistId) => {
  /* istanbul ignore next */
  cov_vfpl5nwab().f[2]++;
  const plan =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[19]++, 
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[11][0]++, plans[userPlan]) ||
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[11][1]++, plans.free));
  /* istanbul ignore next */
  cov_vfpl5nwab().s[20]++;
  if (plan.limits.specialists === "all") {
    /* istanbul ignore next */
    cov_vfpl5nwab().b[12][0]++;
    cov_vfpl5nwab().s[21]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_vfpl5nwab().b[12][1]++;
  }
  cov_vfpl5nwab().s[22]++;
  return plan.limits.specialists.includes(specialistId);
};

// Obter modelo de IA baseado no plano
/* istanbul ignore next */
cov_vfpl5nwab().s[23]++;
export const getAIModel = (userPlan, messageType =
/* istanbul ignore next */
(cov_vfpl5nwab().b[13][0]++, "standard")) => {
  /* istanbul ignore next */
  cov_vfpl5nwab().f[3]++;
  const plan =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[24]++, 
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[14][0]++, plans[userPlan]) ||
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[14][1]++, plans.free));

  // Premium pode escolher modelo avançado
  /* istanbul ignore next */
  cov_vfpl5nwab().s[25]++;
  if (
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[16][0]++, plan.limits.features.advancedModels) &&
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[16][1]++, messageType === "advanced")) {
    /* istanbul ignore next */
    cov_vfpl5nwab().b[15][0]++;
    cov_vfpl5nwab().s[26]++;
    return "gpt-4.1-mini"; // Ou GPT-4.1 se usuário escolher
  } else
  /* istanbul ignore next */
  {
    cov_vfpl5nwab().b[15][1]++;
  }

  // Todos os outros usam Gemini Flash (mais barato)
  cov_vfpl5nwab().s[27]++;
  return "gemini-2.0-flash";
};

// Calcular custo estimado por mensagem
/* istanbul ignore next */
cov_vfpl5nwab().s[28]++;
export const estimateMessageCost = (userPlan, tokens) => {
  /* istanbul ignore next */
  cov_vfpl5nwab().f[4]++;
  const model =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[29]++, getAIModel(userPlan));
  const pricing =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[30]++, {
    "gemini-2.0-flash": 0.000000075,
    // $0.075 / 1M tokens
    "gpt-4.1-mini": 0.0000008 // $0.80 / 1M tokens
  });
  const costPerToken =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[31]++, 
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[17][0]++, pricing[model]) ||
  /* istanbul ignore next */
  (cov_vfpl5nwab().b[17][1]++, pricing["gemini-2.0-flash"]));
  const costUSD =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[32]++, tokens * costPerToken);
  const costBRL =
  /* istanbul ignore next */
  (cov_vfpl5nwab().s[33]++, costUSD * 5); // Conversão aproximada
  /* istanbul ignore next */
  cov_vfpl5nwab().s[34]++;
  return {
    model,
    tokens,
    costUSD,
    costBRL
  };
};
