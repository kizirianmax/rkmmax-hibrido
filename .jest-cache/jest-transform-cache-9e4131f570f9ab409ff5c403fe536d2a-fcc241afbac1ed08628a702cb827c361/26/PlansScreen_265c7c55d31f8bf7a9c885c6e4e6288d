/** Detecta regiÃ£o pelo locale do navegador */
function detectRegion() {
  /* istanbul ignore next */
  cov_ivilfwqze().f[0]++;
  cov_ivilfwqze().s[0]++;
  try {
    const loc =
    /* istanbul ignore next */
    (cov_ivilfwqze().s[1]++, (
    /* istanbul ignore next */
    (cov_ivilfwqze().b[0][0]++, Intl.DateTimeFormat().resolvedOptions().locale) ||
    /* istanbul ignore next */
    (cov_ivilfwqze().b[0][1]++, "pt-BR")).toLowerCase());
    /* istanbul ignore next */
    cov_ivilfwqze().s[2]++;
    return /* istanbul ignore next */(cov_ivilfwqze().b[2][0]++, loc.includes("pt")) ||
    /* istanbul ignore next */
    (cov_ivilfwqze().b[2][1]++, loc.includes("br")) ?
    /* istanbul ignore next */
    (cov_ivilfwqze().b[1][0]++, "BR") :
    /* istanbul ignore next */
    (cov_ivilfwqze().b[1][1]++, "US");
  } catch {
    /* istanbul ignore next */
    cov_ivilfwqze().s[3]++;
    return "BR";
  }
}

