  /**
   * VERIFICAR MÃTRICAS E GERAR ALERTAS
   */
  async checkMetrics(metrics) {
    /* istanbul ignore next */
    cov_ph3q3ema().f[6]++;
    const alerts =
    /* istanbul ignore next */
    (cov_ph3q3ema().s[46]++, []);

    // Memory Usage
    /* istanbul ignore next */
    cov_ph3q3ema().s[47]++;
