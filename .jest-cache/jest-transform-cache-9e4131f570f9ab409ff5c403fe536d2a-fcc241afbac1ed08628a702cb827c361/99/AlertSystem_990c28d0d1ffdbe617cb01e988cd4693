      },
      byComponent: {},
      lastAlert: 
      /* istanbul ignore next */
      (cov_ph3q3ema().b[31][0]++, this.alerts[this.alerts.length - 1]) ||
      /* istanbul ignore next */
      (cov_ph3q3ema().b[31][1]++, null)
    });

    // Contar por componente
    /* istanbul ignore next */
    cov_ph3q3ema().s[75]++;
    this.alerts.forEach(alert => {
      /* istanbul ignore next */
      cov_ph3q3ema().f[19]++;
      const component =
      /* istanbul ignore next */
      (cov_ph3q3ema().s[76]++, 
      /* istanbul ignore next */
      (cov_ph3q3ema().b[32][0]++, alert.component) ||
      /* istanbul ignore next */
      (cov_ph3q3ema().b[32][1]++, "Unknown"));
      /* istanbul ignore next */
      cov_ph3q3ema().s[77]++;
      stats.byComponent[component] = (
      /* istanbul ignore next */
      (cov_ph3q3ema().b[33][0]++, stats.byComponent[component]) ||
      /* istanbul ignore next */
      (cov_ph3q3ema().b[33][1]++, 0)) + 1;
    });
    /* istanbul ignore next */
    cov_ph3q3ema().s[78]++;
    return stats;
  }

  /**
   * HELPER: Obter cor do alerta
   */
  getAlertColor(severity) {
    /* istanbul ignore next */
    cov_ph3q3ema().f[20]++;
    const colors =
    /* istanbul ignore next */
    (cov_ph3q3ema().s[79]++, {
      CRITICAL: "#FF0000",
      WARNING: "#FFA500",
      INFO: "#0099FF"
    });
    /* istanbul ignore next */
    cov_ph3q3ema().s[80]++;
    return /* istanbul ignore next */(cov_ph3q3ema().b[34][0]++, colors[severity]) ||
    /* istanbul ignore next */
    (cov_ph3q3ema().b[34][1]++, "#808080");
  }

