  /**
   * LIMPAR ALERTAS ANTIGOS
   */
  clearOldAlerts(hoursOld =
  /* istanbul ignore next */
  (cov_ph3q3ema().b[30][0]++, 24)) {
    /* istanbul ignore next */
    cov_ph3q3ema().f[16]++;
    const cutoffTime =
    /* istanbul ignore next */
    (cov_ph3q3ema().s[71]++, new Date(Date.now() - hoursOld * 60 * 60 * 1000));
    /* istanbul ignore next */
    cov_ph3q3ema().s[72]++;
    this.alerts = this.alerts.filter(a => {
      /* istanbul ignore next */
      cov_ph3q3ema().f[17]++;
      cov_ph3q3ema().s[73]++;
      return new Date(a.timestamp) > cutoffTime;
    });
  }

  /**
   * OBTER ESTATÃSTICAS
   */
  getStats() {
    /* istanbul ignore next */
    cov_ph3q3ema().f[18]++;
    const stats =
    /* istanbul ignore next */
    (cov_ph3q3ema().s[74]++, {
      totalAlerts: this.alerts.length,
