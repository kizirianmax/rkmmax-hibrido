  /**
   * ATUALIZAR THRESHOLDS
   */
  updateThreshold(metric, value) {
    /* istanbul ignore next */
    cov_ph3q3ema().f[22]++;
    cov_ph3q3ema().s[83]++;
    if (this.thresholds.hasOwnProperty(metric)) {
      /* istanbul ignore next */
      cov_ph3q3ema().b[36][0]++;
      cov_ph3q3ema().s[84]++;
      this.thresholds[metric] = value;
      /* istanbul ignore next */
      cov_ph3q3ema().s[85]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_ph3q3ema().b[36][1]++;
    }
    cov_ph3q3ema().s[86]++;
    return false;
  }

  /**
   * OBTER THRESHOLDS
   */
  getThresholds() {
    /* istanbul ignore next */
    cov_ph3q3ema().f[23]++;
    cov_ph3q3ema().s[87]++;
